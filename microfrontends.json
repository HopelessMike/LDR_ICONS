{
  "$schema": "https://openapi.vercel.sh/microfrontends.json",
  "applications": {
    "v0-website-portfolio": {
      "development": {
        "fallback": "https://v0-website-portfolio.vercel.app"
      }
    },
    "v0-insight-suite": {
      "routing": [
        {
          "group": "insightsuite",
          "paths": ["/InsightSuite", "/InsightSuite/:path*"]
        }
      ]
    },
    "ldr-icons": {
      "packageName": "ldr-icons",
      "routing": [
        {
          "paths": [
            "/ldr-icons",
            "/ldr-icons/:path*"
          ]
        }
      ],
      "development": {
        "fallback": "https://ldr-icons.vercel.app"
      }
    }
  }
}